<html>
<head>
    <meta charset="UTF-8">
    <link href="http://fonts.googleapis.com/css?family=Raleway:400,300,600" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="/css/skeleton.css">
    <link rel="stylesheet" href="/css/normalize.css">
</head>
<body class="container" style="margin-top:2%">
    <h2>Total Hashtag PT</h2>
    <table >
        <thead>
            <tr>
                <th>hashtag</th>
                <th>&nbsp;&nbsp;&nbsp;</th>
                <th>total</th>
            </tr>     
        </thead>
        <tbody>
            <% for(var i = 0; i < pesquisa.length; i++){ %>
            <tr>
                <td><%= pesquisa[i].hashtag%></td>
                <th></th>
                <td style="text-align: right;"><%= pesquisa[i].total%></td>
            </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>